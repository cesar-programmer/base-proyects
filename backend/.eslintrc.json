{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off",
    "eqeqeq": "off",
    "curly": "off",
    "prefer-const": "off",
    "no-var": "off",
    "no-undef": "off"
  }
}
